<databaseChangeLog        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangeloghttp://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">    <changeSet author="Alex.K" id="JIRA-5555">        <createTable tableName="users">            <column autoIncrement="true" name="id" type="BIGINT">                <constraints primaryKey="true"/>            </column>            <column name="username" type="varchar(255)">                <constraints unique="true"/>            </column>            <column name="email" type="varchar(255)">                <constraints unique="true"/>            </column>        </createTable>    </changeSet>    <changeSet author="Alex.K" id="JIRA-5556">        <comment>This change set adds a new column...</comment>        <addColumn tableName="users">            <column name="address" type="varchar(255)"/>        </addColumn>    </changeSet>    <changeSet author="Alex.K" id="JIRA-5557">        <comment>This change set deletes the column that was added in the previous change set</comment>        <dropColumn columnName="address" tableName="users"/>    </changeSet>    <changeSet author="Alex.K" id="JIRA-5558">        <preConditions>            <sqlCheck expectedResult="0">                SELECT COUNT(1) FROM users WHERE username = 'Tim'            </sqlCheck>        </preConditions>        <comment>Add new user</comment>        <insert tableName="users">            <column name="username" value="Jack"/>            <column name="email" value="jack@gmailc.com"/>        </insert>    </changeSet><!--    this change set will not work because of precondition --><!--    <changeSet author="Alex.K" id="JIRA-5559">--><!--        <preConditions>--><!--            <sqlCheck expectedResult="0">--><!--                SELECT COUNT(1) FROM users WHERE username = 'Jack'--><!--        